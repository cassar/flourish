<h1>Membership</h1>
<article>
  <p><strong>Status</strong>: Active</p>
  <ul>
    <li>
      <strong>Email</strong>: 
      <% if current_user.confirmed? %>
        Confirmed 
      <% else%>
        Unconfirmed 
      <% end %>
    </li>
  </ul>
  <p>
    <strong>Pay Outs</strong>: 
    <% if @member.payid.present? %> 
      Enabled
    <% else %>
      Not Enabled
    <% end %>
  </p>
  <ul>
    <li>
      <strong>PayID</strong>: 
      <% if @member.payid.present? %>
        Entered (<%= link_to 'update', edit_payid_path(@member) %>)
      <% else %>
        <%= link_to 'Enter', edit_payid_path(@member) %>
      <% end %>
    </li>
  </ul>
  <p>
    <strong>Contributions</strong>: Enabled
    <ul>
      <li>
        <strong>Reference</strong>:
        <%= @member.contribution_reference %>
      </li>
    </ul>
  </p>
</article>
<nav>
  <%= link_to dividends_path, class: 'button' do %>
    Dividends
  <% end %>
  <%= link_to pay_outs_path, class: 'button' do %>
    Pay Outs
  <% end %>
  <%= link_to edit_user_registration_path, class: 'button' do %>
    Account
  <% end %>
</nav>
